        <arguments>
          <argument>
            <kind>1</kind>
            <string>if state != prevState
{
    stateInit = true;
    image_index = 0;
    prevState = state;
}

switch (state)
{
case st_enemy.SPAWNING:
    if stateInit
        stateInit = false;    
        
    y += 2;
    if (y &gt; stopy)
    {
        state = st_enemy.SPAWNING2;
        image_speed = 0.15;
    }
    break;   
    
case st_enemy.SPAWNING2:
    if stateInit
        stateInit = false;    

    if image_index &gt; image_number - 1
    {
        state = st_enemy.IDLE;
        sprite_index = s_bossAIdle;
        image_index = 0;
        active = true;
    }
    break;
    
case st_enemy.IDLE:
    if stateInit
    {
        sprite_index = s_bossAIdle;
        image_speed = 0.15;
        stateInit = false;
        
        var i = irandom(90) + 30;
        if irandom(1)
            alarm_set(1, i);           // Taunt
        else
            alarm_set(2, i);           // Attack 1
    }
    if (shotTimer == 0)
    {
        shotTimer = 60;
        
        var bul = instance_create(x,y+10,o_enemyBurst);
        //ds_list_add(bullets, bul);
    }
    shotTimer--;
    break;
    
case st_enemy.ATTACK1:
    if stateInit
    {
        sprite_index = s_bossASlam;
        stateInit = false;
    }
    
    if image_index &gt;= 6
    {
        image_speed = 0.15;
    }
    else if image_index &gt;= 5
    {
        image_speed = 0.04;
        c_screen_shake(0.5);
        if slam == noone
        {
            slam = c_create_hitbox(x, y+30, 32, 16, 1, 30, 270, slamTime);
            alarm[3] = slamTime;
        }
        instance_create(irandom(room_width), irandom(238) + 50, o_icicle);
    }
    else if image_index &gt;= 2
        image_speed = 0.5;
    else if image_index &gt;= 1
        image_speed = 0.03;
    
    if image_index &gt; image_number - 1
        state = st_enemy.IDLE;
    
    break;
    
case st_enemy.TAUNT:
    if stateInit
    {
        sprite_index = s_bossARoar;
        stateInit = false;
    }
    
    if image_index &gt;= 4
        image_speed = 0.15;
    else if image_index &gt;= 3
    {
        image_speed = 0.02;
        c_screen_shake(1);
    }
    
    if image_index &gt; image_number - 1
        state = st_enemy.IDLE;
    break;
    
case st_enemy.DEATH:
    if stateInit
    {
        sprite_index = s_bossADeath;
        image_speed = 0.05;
        stateInit = false;
        alarm_set(1, -1);
        alive = false;
    }

    if image_index == 1
        image_speed = 0.15;
    else if image_index &gt; image_number - 1
        instance_destroy();  
    break;
}


if hp &lt;= 0
    state = st_enemy.DEATH;
</string>
